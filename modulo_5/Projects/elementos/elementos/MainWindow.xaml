<Window x:Class="elementos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        
        <Label Content="usuario" HorizontalAlignment="Left" Margin="70,38,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="22" Height="39" Width="87"/>
        <TextBox x:Name="txtusuario" HorizontalAlignment="Left" Height="23" Margin="78,82,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="247" RenderTransformOrigin="0.5,0.5">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.215"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <CheckBox Content="Recordar mi nombre" HorizontalAlignment="Left" Margin="70,128,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.688,0.573" Height="15" Width="129" FontWeight="Bold"/>
        <Button x:Name="txtInicio" Content="Iniciar sesion" HorizontalAlignment="Left" Margin="82,188,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Height="20"/>

    </Grid>
</Window>
